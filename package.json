{
  "name": "hcp-details-demo",
  "version": "1.0.0",
  "description": "HCP Details Demo - Angular application for healthcare professional details",
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --open --port 4200",
    "build": "ng build --configuration production --stats-json && gulp",
    "build:analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/hcp-details-demo/stats.json",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:dist": "npx http-server dist/hcp-details-demo -p 8080 -o"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/hcp-details-demo.git"
  },
  "keywords": [
    "angular",
    "healthcare",
    "hcp",
    "details",
    "demo"
  ],
  "dependencies": {
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "echarts": "^6.0.0",
    "ng-zorro-antd": "^20.3.1",
    "ngx-echarts": "^20.0.2",
    "ngx-scrollbar": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/jasmine": "~5.1.0",
    "del": "^8.0.1",
    "gulp": "^5.0.1",
    "gulp-autoprefixer": "^10.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^3.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-gzip": "^1.4.2",
    "gulp-imagemin": "^9.1.0",
    "gulp-notify": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.1.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "through2": "^4.0.2",
    "typescript": "~5.9.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
